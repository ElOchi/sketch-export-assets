{
    "name": "Sketch Export Assets",
    "description": "Plugins to export assets to different platforms",
    "author": "Geert Wille",
    "homepage": "https://github.com/GeertWille/sketch-export-assets",
    "version": 2.3,
    "identifier": "com.geertwille.sketch.export-assets",
    "compatibleVersion": 3,
    "bundleVersion": 1,
    "commands":
        [
            {
                "name": "Export iOS Assets",
                "identifier": "export-assets-ios",
                "shortcut": "ctrl alt shift 1",
                "script": "Export iOS Assets.sketchscript"
            },
            {
                "name": "Export Android Assets",
                "identifier": "export-assets-android",
                "shortcut": "ctrl alt shift 2",
                "script": "Export Android Assets.sketchscript"
            },
            {
                "name": "Export WindowsPhone Assets",
                "identifier": "export-assets-wp",
                "shortcut": "ctrl alt shift 3",
                "script": "Export WindowsPhone Assets.sketchscript"
            },
            {
                "name": "Export WindowsPhone 8.1 (WinRT) Assets",
                "identifier": "export-assets-winrt",
                "shortcut": "ctrl alt shift 4",
                "script": "Export WinRT Assets.sketchscript"
            },
            {
                "name": "Export Android MIPMAP Assets",
                "identifier": "export-assets-android-mipmap",
                "shortcut": "ctrl alt shift 5",
                "script": "Export Android MIPMAP Assets.sketchscript"
            },
            {
                "name": "Change Settings",
                "identifier": "change-settings",
                "shortcut": "ctrl alt shift 6",
                "script": "Change Settings.sketchscript"
            }
        ],
    "menu":
        {
            "items":
            [
                "export-assets-android",
                "export-assets-ios",
                "export-assets-wp",
                "export-assets-winrt",
                "export-assets-android-mipmap",
                "change-settings"
            ]
        }
}
